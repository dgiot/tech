            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "edit-link": {
                "base": "https://github.com/jaweii/Parse-JavaScript-translation/blob/master",
